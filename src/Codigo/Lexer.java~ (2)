          }
        case 107: break;
        case 20: 
          { lexeme=yytext();return menor;
          }
        case 108: break;
        case 56: 
          { lexeme=yytext();return verify;
          }
        case 109: break;
        case 67: 
          { lexeme=yytext();return resurrect;
          }
        case 110: break;
        case 11: 
          { lexeme=yytext();return mod;
          }
        case 111: break;
        case 17: 
          { lexeme=yytext();return corch_izq;
          }
        case 112: break;
        case 47: 
          { lexeme=yytext();return getY;
          }
        case 113: break;
        case 55: 
          { lexeme=yytext();return invert;
          }
        case 114: break;
        case 29: 
          { lexeme=yytext();return mayor_igual;
          }
        case 115: break;
        case 18: 
          { lexeme=yytext();return corch_der;
          }
        case 116: break;
        case 4: 
          { lexeme=yytext();return line;
          }
        case 117: break;
        case 57: 
          { lexeme=yytext();return compare;
          }
        case 118: break;
        case 62: 
          { lexeme=yytext();return shatter;
          }
        case 119: break;
        case 9: 
          { lexeme=yytext();return multi;
          }
        case 120: break;
        case 21: 
          { lexeme=yytext();return fn;
          }
        case 121: break;
        case 12: 
          { lexeme=yytext();return asig;
          }
        case 122: break;
        case 10: 
          { lexeme=yytext();return div;
          }
        case 123: break;
        case 39: 
          { lexeme=yytext();return main;
          }
        case 124: break;
        case 54: 
          { lexeme=yytext();return enable;
          }
        case 125: break;
        case 42: 
          { lexeme=yytext();return hold;
          }
        case 126: break;
        case 26: 
          { lexeme=yytext();return decremento;
          }
        case 127: break;
        case 5: 
          { lexeme=yytext();return and;
          }
        case 128: break;
        case 6: 
          { lexeme=yytext();return not;
          }
        case 129: break;
        case 25: 
          { lexeme=yytext(); return Entero;
          }
        case 130: break;
        case 23: 
          { lexeme=yytext();return incremento;
          }
        case 131: break;
        case 50: 
          { lexeme=yytext();return podri;
          }
        case 132: break;
        case 24: 
          { lexeme=yytext();return sum_asig;
          }
        case 133: break;
        case 44: 
          { lexeme=yytext();return kill;
          }
        case 134: break;
        case 3: 
          { /*Ignore*/
          }
        case 135: break;
        case 46: 
          { lexeme=yytext();return getX;
          }
        case 136: break;
        case 31: 
          { lexeme=yytext();return dif;
          }
        case 137: break;
        case 69: 
          { lexeme=yytext();return getButton;
          }
        case 138: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            return null;
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


