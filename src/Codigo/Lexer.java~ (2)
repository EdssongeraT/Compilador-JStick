          }
        case 102: break;
        case 1: 
          { lexeme=yytext();return Cadena;
          }
        case 103: break;
        case 63: 
          { lexeme=yytext();return getFunc;
          }
        case 104: break;
        case 45: 
          { lexeme=yytext();return bool;
          }
        case 105: break;
